<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping namespace="WebApplication1" assembly="WebApplication1" xmlns="urn:nhibernate-mapping-2.2">
	<class name="Book" table="Books">
		<id name="Id" column="Id" type="Int32">
			<generator class="native" />
		</id>
		<property name="Name" type="String" column="Name" length="50" />
		<property name="Description" type="String" column="Description" length="50" />
		<property name="ReleaseDate" type="Int32" column="ReleaseDate"/>
		<property name="NumberOfPages" type="Int32" column="NumberOfPages"/>

		<many-to-one name="BookType" class="WebApplication1.Models" column="TypeId" fetch="select" cascade="none"></many-to-one>
	</class>
</hibernate-mapping>